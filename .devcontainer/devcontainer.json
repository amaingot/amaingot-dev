{
  "name": "AMaingot Dev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "containerUser": "coder",
  "onCreateCommand": "/on-create.sh",
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {
      "installZsh": true,
      "configureZshAsDefaultShell": true,
      "installOhMyZsh": true,
      "installOhMyZshConfig": true,
      "upgradePackages": true,
      "username": "coder"
    },
    "ghcr.io/devcontainers/features/git:1": {},
    "ghcr.io/devcontainers/features/git-lfs:1": {},
    "ghcr.io/devcontainers/features/github-cli:1": {},
    "ghcr.io/devcontainers/features/docker-in-docker:2": {
      "version": "latest",
      "installDockerBuildx": true
    },
    "ghcr.io/devcontainers/features/aws-cli:1": {
      "version": "latest"
    },
    "ghcr.io/devcontainers/features/python:1": {
      "installTools": true,
      "version": "3.13"
    },
    "ghcr.io/devcontainers/features/node:1": {
      "nodeGypDependencies": true,
      "installYarnUsingApt": true,
      "version": "lts",
      "pnpmVersion": "latest",
      "nvmVersion": "latest"
    },
    "ghcr.io/va-h/devcontainers-features/uv:1": {
      "shellautocompletion": true,
      "version": "latest"
    },
    "ghcr.io/devcontainers/features/terraform:1": {
      "version": "latest",
      "tflint": "latest",
      "terragrunt": "latest"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "dotjoshjohnson.xml",
        "github.vscode-github-actions",
        "github.vscode-pull-request-github",
        "hashicorp.terraform",
        "marp-team.marp-vscode",
        "mermaidchart.vscode-mermaid-chart",
        "mikestead.dotenv",
        "ms-azuretools.vscode-containers",
        "ms-azuretools.vscode-docker",
        "ms-python.autopep8",
        "ms-python.debugpy",
        "ms-python.pylint",
        "ms-python.python",
        "ms-python.vscode-python-envs",
        "ms-toolsai.jupyter",
        "ms-toolsai.jupyter-keymap",
        "ms-toolsai.jupyter-renderers",
        "ms-toolsai.vscode-jupyter-cell-tags",
        "ms-toolsai.vscode-jupyter-slideshow",
        "openai.chatgpt",
        "prettier.prettier-vscode",
        "redhat.vscode-yaml",
        "streetsidesoftware.code-spell-checker",
        "waderyan.gitblame",
        "wmaurer.change-case",
        "yzhang.markdown-all-in-one"
      ],
      "settings": {
        "editor.acceptSuggestionOnEnter": "on",
        "editor.dragAndDrop": false,
        "workbench.colorTheme": "Default Dark+",
        "editor.formatOnSave": true,

        "editor.fontLigatures": true,
        "editor.inlineSuggest.enabled": true,
        "editor.minimap.enabled": false,
        "editor.renderWhitespace": "all",
        "editor.snippetSuggestions": "top",
        "editor.suggest.preview": true,
        "editor.suggest.showMethods": true,
        "editor.tabSize": 2,
        "editor.stickyScroll.enabled": false,

        "update.showReleaseNotes": false,
        "workbench.startupEditor": "none",
        "security.workspace.trust.untrustedFiles": "open",

        "explorer.confirmDelete": false,
        "explorer.confirmDragAndDrop": false,

        "terminal.integrated.enableMultiLinePasteWarning": "never",
        "terminal.integrated.defaultProfile.osx": "zsh",

        "extensions.autoUpdate": true,
        "extensions.ignoreRecommendations": true,

        "files.autoSave": "afterDelay",
        "files.trimTrailingWhitespace": true,

        "git.allowNoVerifyCommit": true,
        "git.autofetch": true,
        "git.confirmNoVerifyCommit": false,
        "git.confirmSync": false,
        "git.enableSmartCommit": true,
        "git.replaceTagsWhenPull": true,
        "diffEditor.ignoreTrimWhitespace": false,

        "[javascript]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode"
        },
        "javascript.updateImportsOnFileMove.enabled": "never",

        "[typescript]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode"
        },
        "typescript.preferences.importModuleSpecifier": "relative",
        "typescript.updateImportsOnFileMove.enabled": "always",
        "[typescriptreact]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode"
        },

        "[json]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode"
        },
        "[jsonc]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode"
        },

        "[html]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode"
        },
        "html.format.wrapLineLength": 80,

        "[css]": {
          "editor.defaultFormatter": "vscode.css-language-features"
        },

        "[yaml]": {
          "editor.defaultFormatter": "redhat.vscode-yaml"
        },
        "redhat.telemetry.enabled": false,

        "[graphql]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode"
        },

        "[markdown]": {
          "editor.defaultFormatter": "yzhang.markdown-all-in-one"
        },

        "[handlebars]": {
          "editor.defaultFormatter": "vscode.html-language-features"
        },

        "[python]": {
          "editor.tabSize": 4,
          "editor.defaultFormatter": "ms-python.autopep8"
        },
        "python.analysis.typeCheckingMode": "standard",
        "python.languageServer": "Pylance",
        "pylint.importStrategy": "fromEnvironment",
        "autopep8.importStrategy": "fromEnvironment",

        "jupyter.askForKernelRestart": false,
        "notebook.lineNumbers": "on",
        "notebook.defaultFormatter": "ms-python.autopep8",
        "notebook.diff.overviewRuler": true,
        "notebook.formatOnSave.enabled": true,
        "notebook.output.scrolling": true,
        "notebook.output.wordWrap": true,

        "terraform.validation.enableEnhancedValidation": true,

        "cSpell.userWords": [
          "aiodataloader",
          "allthedata",
          "amaingot",
          "apos",
          "Appbar",
          "auctane",
          "authed",
          "authlib",
          "Autoscaler",
          "autosize",
          "AWSX",
          "axelspringer",
          "backoff",
          "bigquery",
          "bioguide",
          "browserslist",
          "cachetools",
          "Chargebacks",
          "ciphertext",
          "clsx",
          "codegen",
          "Dashenforcers",
          "Datagrid",
          "dateutil",
          "Democritas",
          "documentai",
          "dotenv",
          "dropin",
          "ecommerce",
          "endicia",
          "expressjwt",
          "firestore",
          "fitz",
          "frauddetector",
          "googleapis",
          "graphql",
          "headshot",
          "icps",
          "imagebuilder",
          "inmemory",
          "ionicons",
          "JAAS",
          "JSONB",
          "jwks",
          "kafkajs",
          "kanban",
          "kazoohr",
          "Kibana",
          "kubectl",
          "kubernetes",
          "letsencrypt",
          "livemode",
          "llms",
          "Maingot",
          "managedwriter",
          "mangum",
          "Manske",
          "mantine",
          "MAPBOX",
          "markdownify",
          "metabase",
          "mgmt",
          "mixins",
          "monoauth",
          "multivac",
          "netsuite",
          "newrelic",
          "nextjs",
          "ntrp",
          "oidc",
          "onboarded",
          "onupdate",
          "openai",
          "OPENSEARCH",
          "opentelemetry",
          "Paginator",
          "phonenumber",
          "Polyauth",
          "postgres",
          "postoak",
          "powertools",
          "psycopg",
          "recieved",
          "Recurly",
          "refetch",
          "reindex",
          "resave",
          "respawn",
          "rollbar",
          "sasl",
          "Searchbar",
          "sendgrid",
          "sessionmaker",
          "setex",
          "shipengine",
          "shippingeasy",
          "Shipstation",
          "signup",
          "sqlalchemy",
          "sqltext",
          "starlette",
          "Superadmin",
          "Swipeable",
          "swsim",
          "tabler",
          "talentiq",
          "tamagui",
          "teamwhat",
          "timeframe",
          "Timeloop",
          "TIMESTREAM",
          "Todos",
          "TWILIO",
          "typeorm",
          "Unauthed",
          "unidecode",
          "uvicorn",
          "waitlist",
          "weaviate",
          "webservice",
          "websockets",
          "workshift",
          "workshifts",
          "xmltodict",
          "zyte"
        ],

        "remote.autoForwardPortsSource": "hybrid",
        "docker.extension.enableComposeLanguageServer": false,

        "markdown.marp.html": "all",
        "github.copilot.nextEditSuggestions.enabled": true,
        "python.defaultInterpreterPath": "/opt/homebrew/bin/python3.12",
        "githubPullRequests.pullBranch": "never",

        "workbench.editorAssociations": {
          "*.copilotmd": "vscode.markdown.preview.editor",
          "*.svg": "default"
        },
        "[dockercompose]": {
          "editor.insertSpaces": true,
          "editor.tabSize": 2,
          "editor.autoIndent": "advanced",
          "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
          },
          "editor.defaultFormatter": "redhat.vscode-yaml"
        },
        "[github-actions-workflow]": {
          "editor.defaultFormatter": "redhat.vscode-yaml"
        },
        "python.terminal.useEnvFile": true,
        "editor.unicodeHighlight.ambiguousCharacters": false
      }
    }
  }
}
